{
  "name": "datatunerx-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "npm run lint:style && npm run lint:es && npm run lint:type",
    "change:version": "npx esno ./scripts/change-version.ts",
    "commit": "git-cz",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "lint:es": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\" --config .eslintrc.js --fix --max-warnings=0",
    "lint:style": "stylelint \"src/**/*.{vue,scss,css}\" --config .stylelintrc.js --fix",
    "lint:type": "vue-tsc -noEmit",
    "postinstall": "lint-inject-vscode && i18n-inject-vscode",
    "report": "vue-cli-service build --report",
    "test:unit:ci": "vue-cli-service test:unit --ci --coverage --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@antv/g2": "^5.1.10",
    "@dao-style/core": "1.9.0",
    "@dao-style/extend": "1.7.2-beta.3",
    "@types/showdown": "^2.0.6",
    "@vueuse/core": "^8.5.0",
    "@vueuse/shared": "^8.5.0",
    "axios": "^1.6.2",
    "clipboard-copy": "^4.0.1",
    "core-js": "^3.33.3",
    "dayjs": "1.11.10",
    "github-markdown-css": "^5.5.0",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "marked": "^11.1.0",
    "marked-highlight": "^2.0.9",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "vee-validate": "^4.12.3",
    "vue": "^3.3.8",
    "vue-dompurify-html": "^5.0.0",
    "vue-i18n": "9.7.1",
    "vue-router": "^4.2.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/cz-commitlint": "^16.0.0",
    "@dao-style/unplugin-extend": "~0.0.6",
    "@dao-style/vue-cli-plugin-ci": "^0.1.7",
    "@dao-style/vue-cli-plugin-dao-style": "^0.0.14",
    "@dao-style/vue-cli-plugin-i18n": "^0.0.10",
    "@dao-style/vue-cli-plugin-lint": "^0.0.25",
    "@dao-style/vue-cli-plugin-pinia": "^0.0.8",
    "@dao-style/vue-cli-plugin-router": "^0.0.6",
    "@dao-style/vue-cli-plugin-tailwind": "~0.0.4",
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@types/json-patch": "^0.0.33",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.3.1",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^27.0.6",
    "commitizen": "^4.2.4",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-vue": "^8.4.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "esno": "^0.16.3",
    "jest": "^27.0.5",
    "jest-junit": "^13.0.0",
    "kubernetes-types": "^1.26.0",
    "lint-staged": "^12.3.2",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "semver": "^7.3.8",
    "stylelint": "^15.6.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.17.2",
    "utility-types": "^3.10.0",
    "vue-tsc": "^1.2.0",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  }
}
